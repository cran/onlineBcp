Package: onlineBcp
Type: Package
Title: Online Bayesian Methods for Change Point Analysis
Version: 0.1.2
Authors@R: c(
    person("Hongyan", "Xu", email = "hxu@augusta.edu", role = c("cre", "aut")),
    person("Ayten", "Yigiter", email = "yigiter@hacettepe.edu.tr", role = "aut"),
